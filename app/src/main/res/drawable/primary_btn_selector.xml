<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Button不可用时 -->
    <item android:drawable="@drawable/primary_btn_bg_enabled" android:state_enabled="false"/>
    <!-- Button正常状态下的背景 -->
    <item android:drawable="@drawable/primary_btn_bg_normal" android:state_pressed="false"/>
    <!-- Button按下时的背景 -->
    <item android:drawable="@drawable/primary_btn_bg_pressed" android:state_pressed="true"/>

</selector>